{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [
    "VITE_CLERK_PUBLISHABLE_KEY",
    "VITE_AWS_API_INVOKE_URL",
    "VITE_SERVER_URL",
    "ALLOWED_ORIGINS"
  ],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "@cmumaps/server#db-generate": {},
    "@cmumaps/server#build": {
      "dependsOn": [
        "@cmumaps/server#build",
        "@cmumaps/server#db-generate"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "start": {
      "dependsOn": [
        "^start",
        "^build"
      ]
    },
    "format": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "cache": false,
      "persistent": true
    }
  }
}