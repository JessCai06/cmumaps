{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "start": {
      "dependsOn": ["^start", "^@cmumaps/server#db-generate", "^build"]
    },
    "@cmumaps/server#db-generate": {},
    "build": {
      "dependsOn": ["^build", "@cmumaps/server#db-generate"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".expo/**",
        ".output/**",
        ".vercel/output/**",
        "dist/**",
        "build/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
